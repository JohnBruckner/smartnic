+  __asm__ __volatile__ ("isb; mrs %0, cntvct_el0" : "=r" (var) )
+#define HP_TIMING_NOW_P(var) \
+  __asm__ __volatile__ ("isb; mrs %0, cntpct_el0" : "=r" (var) )
